            <Font name="Segoe UI Bold Italic" size="24.0" />
         </font>
         <effect>
            <InnerShadow height="10.0" radius="4.5" width="10.0">
               <color>
                  <Color red="0.1103999987244606" green="0.6501333117485046" blue="0.9200000166893005" />
               </color>
            </InnerShadow>
         </effect>
      </Label>
      <Label layoutX="271.0" layoutY="331.0" onMouseClicked="#newAccount" prefHeight="37.0" prefWidth="128.0" text="Registrarse" textFill="WHITE">
         <font>
            <Font name="Segoe UI Bold Italic" size="24.0" />
         </font>
         <effect>
            <InnerShadow height="10.0" radius="4.5" width="10.0">
               <color>
                  <Color red="0.1103999987244606" green="0.6501333117485046" blue="0.9200000166893005" />
               </color>
            </InnerShadow>
         </effect>
      </Label>
